<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.162-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.naoth.rc.dialogs.multiagentconfiguration.ui.RepresentationsTab">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
         <items>
            <Button fx:id="btnUpdateList" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#updateList" styleClass="refresh_button" text="Update" />
            <ChoiceBox fx:id="type" prefWidth="150.0" />
         </items>
      </ToolBar>
      <SplitPane dividerPositions="0.2" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <items>
            <ListView fx:id="list" prefHeight="200.0" prefWidth="200.0" />
            <TextArea fx:id="content" editable="false" prefHeight="200.0" prefWidth="200.0" />
         </items>
      </SplitPane>
   </children>
</VBox>
