<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.Tooltip?>

<VBox prefHeight="578.0" prefWidth="756.0" stylesheets="@ui/style.css" xmlns="http://javafx.com/javafx/8.0.162-ea" xmlns:fx="http://javafx.com/fxml/1">
  <children>
      <AnchorPane prefHeight="562.0" prefWidth="756.0" VBox.vgrow="ALWAYS">
         <children>
            <TabPane fx:id="tabpane" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            <HBox layoutX="724.0" layoutY="3.0" spacing="4.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0">
               <children>
                  <Button fx:id="btnDisconnectAll" mnemonicParsing="false" onAction="#disconnectAll" prefWidth="29.0" text="-" textAlignment="CENTER">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                     <tooltip>
                       <Tooltip text="Disconnect from all robots."/>
                     </tooltip>
                  </Button>
                  <Button fx:id="btn_add" mnemonicParsing="false" onAction="#addRobots" text="+" textAlignment="CENTER">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                     <tooltip>
                       <Tooltip text="Connect to additional robots."/>
                     </tooltip>
                  </Button>
               </children>
            </HBox>
         </children>
      </AnchorPane>
  </children>
</VBox>
