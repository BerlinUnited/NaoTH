<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane xmlns="http://javafx.com/javafx/8.0.202-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.naoth.rc.dialogsFx.moduleconfiguration.ModuleConfigurationModules">
   <children>
      <TreeView fx:id="moduleTree" disable="true" onKeyPressed="#fxListShortcuts" showRoot="false" />
      <Label fx:id="notice" style="-fx-background-color: transparent;" textFill="#13ae18" visible="false" StackPane.alignment="TOP_RIGHT">
         <StackPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </StackPane.margin>
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="search" focusTraversable="false" maxWidth="150.0" promptText="Search" visible="false" StackPane.alignment="BOTTOM_RIGHT">
         <StackPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </StackPane.margin>
      </TextField>
   </children>
</StackPane>
