{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/build/" : true
    },
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "cquery.cacheDirectory": "${workspaceFolder}/.vscode/cquery_cached_index/",
    "cquery.showDocumentLinksOnIncludes": false,
    "cquery.diagnostics.onParse": false,
    "cquery.diagnostics.onCodeCompletion": false
}