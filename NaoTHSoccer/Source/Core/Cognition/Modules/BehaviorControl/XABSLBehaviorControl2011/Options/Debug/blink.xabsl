option blink
{
  initial state on
  {

    decision{
      if(state_time > 1000)
        goto off;
      else
        stay;
    }
    action
    {
      set_left_ear = 10;
      set_right_ear = 10;
    }
  }


  state off
  {
    decision{
      if(state_time > 1000)
        goto on;
      else
        stay;
    }
    action
    {
      set_left_ear = 0;
      set_right_ear = 0;
    }
  }

}