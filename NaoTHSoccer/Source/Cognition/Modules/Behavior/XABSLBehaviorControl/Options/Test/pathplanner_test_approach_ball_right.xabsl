/** Test behavior for the PrimitiveManeuvers module */
option pathplanner_test_approach_ball_right {
  initial state test {    
    action {
    
      // react to the game controller and handle fall down
      basic_play();

      pathplanner_approach_ball_right();
      
      arm.control_mode = back;
      // control the arms
      arms_control();

      // control head movement according to value of head.control_mode
      head_control();
    }
  }
}