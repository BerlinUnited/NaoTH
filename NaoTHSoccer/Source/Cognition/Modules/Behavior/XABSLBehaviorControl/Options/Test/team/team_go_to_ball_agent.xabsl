/** Go to ball if striker */
option team_go_to_ball_agent {

  initial state test {    
    action {
    
      // react to the game controller and handle fall down
      basic_play();

      team_go_to_ball();
      
      arms_control();
      
      // control head movement according to value of head.control_mode
      head_control();
    }
  }
}
