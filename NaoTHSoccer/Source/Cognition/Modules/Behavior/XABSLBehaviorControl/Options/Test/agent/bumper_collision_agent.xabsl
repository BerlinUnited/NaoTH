option bumper_collision_agent {

  initial state test {    
    action {
    
      // react to the game controller and handle fall down
      basic_play();

      take_arms_back();

      bumper_collision_stop();

      // control the arms
      arms_control();
      
      // control head movement according to value of head.control_mode
      head_control();
      
      head.angleVelocity = 20;
    }
  }
}
