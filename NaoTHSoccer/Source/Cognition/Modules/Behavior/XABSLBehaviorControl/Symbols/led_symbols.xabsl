/** Symbols for accessing Leds */
namespace led_symbols("Led Symbols") {

 /* Enumeration for possible values of the eye leds  */
  enum eyeLED
  {
    off,
    red,
    green,
    blue,
    red_green
  };

  /** values 0-9 possible */
  float output set_right_ear;
  /** values 0-9 possible */
  float output set_left_ear; 

  /** values 0-6 possible  (0 nil, 6 all) */
  float output set_head;
  
  enum eyeLED output set_right_eye;
  enum eyeLED output set_left_eye;  
}
  
